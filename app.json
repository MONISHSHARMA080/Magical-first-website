{
  "expo": {
    "name": "generate-website",
    "slug": "Magical-first-website",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "magicalfirstwebsite",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/generate-website.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Magicalfirstwebsite",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/generate-website.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Magicalfirstwebsite"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/generate-website.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
        }
      ],
      "expo-secure-store"
    ],
    "env": {
      "EXPO_PUBLIC_PRODUCTION": "true",
      "EXPO_PUBLIC_GOOGLE_CLIENT_ID": "536289115131-99enca0gqcqjhd3a3ijtji049e0th543.apps.googleusercontent.com",
      "EXPO_PUBLIC_Spotify_CLIENT_ID": "812c827d57b44b2497941ccb210ae022",
      "EXPO_PUBLIC_BACKEND_URL": "https://generate-a-website.fly.dev",
      "EXPO_PUBLIC_SVELTE_URL": "https://generate-website.fly.dev",
      "EXPO_PUBLIC_VEXO_API_KEY": "d2fa3b3d-2a9c-4173-b598-85d5b3906bf7"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f9db532d-85b3-4371-9a26-8cf846e3d293"
      }
    }
  }
}