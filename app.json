{
  "expo": {
    "name": "generate-website",
    "slug": "Magical-first-website",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "magicalfirstwebsite",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/generate-website.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Magicalfirstwebsite",
      "config": {
        "usesNonExemptEncryption": false  
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/generate-website.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Magicalfirstwebsite"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/generate-website.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f9db532d-85b3-4371-9a26-8cf846e3d293"
      }
    }
  }
}
